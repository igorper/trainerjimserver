# Ignore bundler config and locally bundled gems
/.bundle
vendor/bundle

# Ignore compiled assets
public/assets

# Ignore per-server-instance data
public/system

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/out/
/.capistrano/

# Ignore sublime project files
*.sublime-project
*.sublime-workspace

# IntelliJ's stuff
.idea/
*.iml

# NPM stuff
node_modules/