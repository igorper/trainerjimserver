<% if @measurements.empty? %>
  
Uporabnik se ni treniral.

<% else %>
  <div class="calendar">
    <div class="btn-toolbar">
      <div class="btn-group">
        <button class="btn">Prev</button>
        <button class="btn disabled"><%= displayMonth(@measurements.first.start_time) %></button>
        <button class="btn">Next</button>
        </div>
      
      <div class="btn-group">
          <% @measurements.group_by{ |measurement| measurement.start_time.day}.each do |k,v| %>
       <button class="btn dayelement" measurement-id="<%=v.first.id %>"><%= k %> </button>
          <% end  %>
      </div>
    </div>
    

  </div>
  <br>
  <div id="graphWindow">
    <div id="placeholder"></div>
    <div id="workoutSets">
    </div>
  </div>
  
  
  <div id="types"></div>
  
<% end %>